<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.Toolbar xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="70dp"
    android:background="#57C28C"
    android:id="@+id/toolbar">
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#57C28C"
        android:id="@+id/arrow10"
        android:layout_marginTop="20sp"
        android:src="@drawable/ic_baseline_arrow_back_241"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Report A Tip"
        android:textSize="30sp"
        android:textStyle="bold"
        android:id="@+id/reportbar"
        android:layout_toRightOf="@id/arrow10"
        android:textColor="#EFEEF1"
        android:padding="10sp"/>
</androidx.appcompat.widget.Toolbar>